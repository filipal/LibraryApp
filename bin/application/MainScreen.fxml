<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="193.0" prefWidth="198.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainController">
   <children>
      <AnchorPane layoutY="-1.0" prefHeight="361.0" prefWidth="351.0">
         <children>
            <Label alignment="CENTER" layoutX="115.0" layoutY="14.0" prefHeight="15.0" prefWidth="143.0" text="BiblioTech" textOverrun="CLIP">
               <font>
                  <Font name="Roboto Bold" size="16.0" />
               </font>
            </Label>
            <Button fx:id="addBookButton" layoutX="137.0" layoutY="89.0" mnemonicParsing="false" onAction="#openAddBookDialog" prefHeight="27.0" prefWidth="99.0" text="Add Book">
               <font>
                  <Font name="Roboto" size="14.0" />
               </font>
            </Button>
            <Button fx:id="borrowBookButton" layoutX="137.0" layoutY="143.0" mnemonicParsing="false" onAction="#openBorrowBookDialog" prefHeight="27.0" prefWidth="99.0" text="Borrow Book">
               <font>
                  <Font name="Roboto" size="14.0" />
               </font>
            </Button>
            <Button fx:id="returnBookButton" layoutX="137.0" layoutY="200.0" mnemonicParsing="false" onAction="#openReturnBookDialog" prefHeight="27.0" prefWidth="99.0" text="Return Book">
               <font>
                  <Font name="Roboto" size="14.0" />
               </font>
            </Button>
            <Button fx:id="exitButton" layoutX="131.0" layoutY="309.0" mnemonicParsing="false" onAction="#exitApplication" prefHeight="38.0" prefWidth="111.0" text="Exit BiblioTech" textFill="#c70404">
               <font>
                  <Font name="Roboto Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
